@page
@model HolaGuide.Pages.Admin.CourseModel
@{
}

<script src="~/js/Admin.js"></script>

<form id="myForm" asp-page="/Admin/Course" method="post" enctype="multipart/form-data">
    <input type="text" name="name" placeholder="Service Name" />
    <input type="text" name="price" placeholder="Service Price" />
    <input type="text" name="title" placeholder="Service Title" />
    <input type="text" name="categoryId" placeholder="Category ID" />
    <input type="text" name="longitude" placeholder="Longitude" />
    <input type="text" name="latitude" placeholder="Latitude" />
    <input type="text" name="description" placeholder="Location description" />
    <input type="text" name="contactNumber" placeholder="Service contact number" />
    <input type="file" name="serviceImages" multiple required />
    <input type="submit" value="submit"/>
</form>
