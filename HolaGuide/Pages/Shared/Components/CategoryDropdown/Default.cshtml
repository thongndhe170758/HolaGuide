@{
    @using Models.SQLServer;
    @model List<Category>
}

<select class="form-control w-100" name="category">
    <option value="">-- Danh Mục --</option>
    @foreach (var category in Model)
    {
        <option value="@category.Id">@category.Name</option>
    }
</select>